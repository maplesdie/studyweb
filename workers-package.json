{
  "name": "study-api-worker",
  "version": "1.0.0",
  "description": "Cloudflare Workers API for study records with D1 database",
  "main": "src/worker.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "tail": "wrangler tail",
    "db:create": "wrangler d1 create study-database",
    "db:migrate": "wrangler d1 execute study-database --file=./migrations/schema.sql",
    "db:migrate-local": "wrangler d1 execute study-database --local --file=./migrations/schema.sql"
  },
  "devDependencies": {
    "wrangler": "^3.0.0"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "d1",
    "api",
    "study"
  ],
  "author": "xiugou",
  "license": "MIT"
}